<h1>All tests</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th>Questions</th>
    <th>Action</th>
  </tr>
  <% @tests.each do |test| %>
  <tr>
    <td><%= test.title %></td>
    <td><%= test.level %></td>
    <td><%= test.questions.count %></td>
    <td><%= link_to 'Show', test_path(test) %></td>
  </tr>
  <% end %>
</table>
